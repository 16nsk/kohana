		"php": ">=5.4.0",
		"kohana/core": "3.4.*",
		"kohana/auth": "3.4.*",
		"kohana/cache": "3.4.*",
		"kohana/codebench": "3.4.*",
		"kohana/database": "3.4.*",
		"kohana/image": "3.4.*",
		"kohana/minion": "3.4.*",
		"kohana/orm": "3.4.*"
	},
	"require-dev": {
